{
  "name": "backyard",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "yarn port:check && env-cmd -f .env.development yarn db:setup && env-cmd -f .env.development next dev -p 3000",
    "db:setup": "NODE_ENV=$NODE_ENV prisma db push && prisma generate",
    "build": "env-cmd -f .env.production yarn db:setup && next build",
    "start": "env-cmd -f .env.production next start -p ${PORT:-3000}",
    "build:local": "env-cmd -f .env.development yarn build",
    "start:local": "env-cmd -f .env.development next start -p 3000",
    "migrate:deploy": "prisma migrate deploy",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "coverage:report": "vitest run --coverage && node scripts/coverage-report.js",
    "port:check": "node scripts/check-port.js",
    "webhook:test": "node scripts/test-webhook.js"
  },
